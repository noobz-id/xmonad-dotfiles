{
  "name": "coc-clang-format-style-options",
  "version": "0.0.5",
  "description": "coc.nvim extension, helps you write `.clang-format` more easily.",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/voldikss/coc-clang-format-style-options"
  },
  "publisher": "voldikss",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.74"
  },
  "scripts": {
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "properties": {
        "clang-format-style-options.shortcut": {
          "type": "string",
          "default": "voldikss"
        },
        "clang-format-style-options.priority": {
          "type": "number",
          "default": 99
        }
      }
    }
  },
  "author": "dyzplus@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@voldikss/tsconfig": "^1.0.0",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.18.0",
    "coc.nvim": "^0.0.80",
    "colors": "^1.4.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0"
  }
}
